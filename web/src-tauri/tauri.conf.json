{
    "productName": "OussamaAuto",
    "version": "0.1.2",
    "identifier": "com.OussamaAuto.desktop",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:3000",
        "frontendDist": "../out"
    },
    "bundle": {
        "active": true,
        "targets": [
            "nsis",
            "msi"
        ],
        "icon": [
            "icons/icon.ico"
        ],
        "publisher": "Oussama Auto",
        "resources": [],
        "windows": {
            "certificateThumbprint": null,
            "digestAlgorithm": "sha256",
            "timestampUrl": ""
        }
    },
    "plugins": {
        "updater": {
            "pubkey": "",
            "endpoints": [
                "https://github.com/mohamedrouini02-arch/oussama-auto/releases/latest/download/latest.json"
            ],
            "windows": {
                "installMode": "passive"
            }
        }
    },
    "app": {
        "windows": [
            {
                "title": "OussamaAuto",
                "width": 1200,
                "height": 800,
                "resizable": true,
                "fullscreen": false,
                "label": "main"
            }
        ],
        "security": {
            "csp": null
        },
        "withGlobalTauri": true
    }
}